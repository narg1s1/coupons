{
  "name": "@pe/coupons",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run i18n:serve && ng serve --port=8080",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production node ./node_modules/@angular/cli/bin/ng build --project=prod --prod --prod --deploy-url /dist_ext/coupons/ && npm run i18n:prod",
    "test": "ng test",
    "cli": "pe-i18n-cli",
    "i18n:prod": "rimraf dist/i18n && I18N_HOST=translate.payever.de pe-i18n-cli push --domain-prefix=coupons && I18N_HOST=translate.payever.de pe-i18n-cli pull --output-path=./dist/i18n --domain-prefix=coupons",
    "i18n:serve": "npm run cli ngcli_i18n && pe-i18n-cli push --domain-prefix=coupons && pe-i18n-cli pull --output-path=./i18n-serve --domain-prefix=coupons"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.3.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "6.x",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@pe/ng-kit": "6.24.0-dev-kozak-version-2",
    "@pe/ui-kit": "6.x",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/lodash": "^4.14.109",
    "@types/jquery": "3.3.2",
    "@pe/cli": "latest",
    "typescript": "2.7.2",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
